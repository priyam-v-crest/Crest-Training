   "source": [
    "## Count the frequency of the elements of a given unordered list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "a0a02c8f",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[2, 2, 5, 3, 4, 1, 3]"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from itertools import groupby\n",
    "\n",
    "def count_freq(l):\n",
    "    l.sort()\n",
    "    return [len(list(group)) for key, group in groupby(l)]\n",
    "\n",
    "unordered_list = [2,1,3,8,5,1,4,2,3,4,0,8,2,0,8,4,2,3,4,2]\n",
    "count_freq(unordered_list)"
   ]
  },
