   "source": [
    "## Check if a Number is Perfect"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "73a85f4b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "True\n"
     ]
    }
   ],
   "source": [
    "def is_perfect(n):\n",
    "    divisors = []\n",
    "    for i in range(1,n//2+1):\n",
    "        if n%i == 0:\n",
    "            divisors.append(i)\n",
    "    return sum(divisors) == n\n",
    "\n",
    "n = 8128\n",
    "print(is_perfect(n))"
   ]
  },
