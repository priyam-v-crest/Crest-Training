   "source": [
    "##  Frozenset of squares of odd numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b5ffb2b1",
   "metadata": {},
   "outputs": [],
   "source": [
    "def frozen_odd_squares(n):\n",
    "    return frozenset({x**2 for x in range(1,n,2)})"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "aa21db7e",
