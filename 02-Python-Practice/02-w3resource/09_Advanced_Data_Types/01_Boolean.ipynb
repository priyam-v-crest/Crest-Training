   "source": [
    "## Point-in-Circle Checker"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "b1bccb98",
   "metadata": {},
   "outputs": [],
   "source": [
    "import math\n",
    "def ispointincircle(r, x1, y1, x2, y2):\n",
    "    return math.sqrt((x1-x2)**2 + (y1-y2)**2) < r"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "6a0065a0",
   "metadata": {},
